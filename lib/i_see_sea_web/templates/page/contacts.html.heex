<div class="relative flex flex-col w-10/12 justify-center items-center h-auto">
  <div class="flex items-center justify-center h-16 mb-5">
    <p class="font-normal text-[#189ab4] text-5xl"><%= t!(@locale, "Contact with:") %></p>
  </div>

  <div class="flex flex-col justify-center items-center md:flex-row md:justify-center w-full h-full flex-wrap md:mt-7">
    <div class="flex flex-col items-center justify-center w-9/12 md:w-7/12 md:h-full h-[325px] rounded-md bg-[#e7f5f7]">
      <img
        class="bounce-message mb-5"
        src="data:image/png;base64,"
        alt="message"
      />
      <p class="text-[#189ab4] text-center">
        <%= t!(@locale, "IARA - Fisheries and Aquaculture Executive Agency") %>
      </p>
      <p class="text-[#189ab4] text-center"><%= t!(@locale, "IARA - contact form page") %></p>
      <button class="font-normal text-white rounded-md cursor-pointer w-7/12 h-12 bg-[#007bff] hover:bg-[#0056b3] my-3">
        <%= t!(@locale, "IARA - contact form") %>
      </button>
    </div>
  </div>
</div>
